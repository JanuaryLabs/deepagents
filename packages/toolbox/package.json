{
  "name": "@deepagents/toolbox",
  "version": "0.19.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/JanuaryLabs/deepagents",
    "directory": "packages/toolbox"
  },
  "bugs": {
    "url": "https://github.com/JanuaryLabs/deepagents/issues"
  },
  "homepage": "https://github.com/JanuaryLabs/deepagents#readme",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/lib/*",
      "import": "./dist/lib/*",
      "default": "./dist/lib/*"
    }
  },
  "files": [
    "dist",
    "!**/*.tsbuildinfo",
    "!**/*.test.*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@ai-sdk/groq": "^3.0.24",
    "@deepagents/agent": "0.19.0",
    "ai": "^6.0.90",
    "duck-duck-scrape": "^2.2.7",
    "lodash-es": "^4.17.21",
    "nano-spawn": "^2.0.0",
    "zod": "^3.25.76 || ^4.0.0"
  },
  "devDependencies": {
    "@types/debug": "^4.1.12"
  }
}
