{
  "name": "@deepagents/toolbox",
  "version": "0.0.1",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/lib/*",
      "import": "./dist/lib/*",
      "default": "./dist/lib/*"
    }
  },
  "files": [
    "dist",
    "!**/*.tsbuildinfo",
    "!**/*.test.*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "ai": "^5.0.65",
    "nano-spawn": "^1.0.3",
    "zod": "^3.25.76 || ^4.0.0",
    "duck-duck-scrape": "^2.2.7",
    "lodash-es": "^4.17.21",
    "@ai-sdk/groq": "2.0.23",
    "@deepagents/agent": "0.0.1"
  },
  "peerDependencies": {},
  "devDependencies": {
    "@types/debug": "^4.1.12"
  }
}
