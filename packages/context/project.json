{
  "name": "@deepagents/context",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/context/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/esbuild:esbuild",
      "outputs": ["{options.outputPath}"],
      "options": {
        "additionalEntryPoints": ["packages/context/src/browser.ts"]
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "dependsOn": ["build"],
      "options": {
        "command": "node --test",
        "cwd": "{projectRoot}"
      }
    }
  }
}
