{
  "name": "@deepagents/text2sql",
  "version": "0.6.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/JanuaryLabs/deepagents",
    "directory": "packages/text2sql"
  },
  "bugs": {
    "url": "https://github.com/JanuaryLabs/deepagents/issues"
  },
  "homepage": "https://github.com/JanuaryLabs/deepagents#readme",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "development": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./instructions": {
      "development": "./src/lib/instructions.ts",
      "types": "./dist/lib/instructions.d.ts",
      "import": "./dist/lib/instructions.js",
      "default": "./dist/lib/instructions.js"
    },
    "./grounding": {
      "development": "./src/lib/adapters/groundings/index.ts",
      "types": "./dist/lib/adapters/groundings/index.d.ts",
      "import": "./dist/lib/adapters/groundings/index.js",
      "default": "./dist/lib/adapters/groundings/index.js"
    },
    "./sqlite": {
      "development": "./src/lib/adapters/sqlite/index.ts",
      "types": "./dist/lib/adapters/sqlite/index.d.ts",
      "import": "./dist/lib/adapters/sqlite/index.js",
      "default": "./dist/lib/adapters/sqlite/index.js"
    },
    "./postgres": {
      "development": "./src/lib/adapters/postgres/index.ts",
      "types": "./dist/lib/adapters/postgres/index.d.ts",
      "import": "./dist/lib/adapters/postgres/index.js",
      "default": "./dist/lib/adapters/postgres/index.js"
    },
    "./sqlserver": {
      "development": "./src/lib/adapters/sqlserver/index.ts",
      "types": "./dist/lib/adapters/sqlserver/index.d.ts",
      "import": "./dist/lib/adapters/sqlserver/index.js",
      "default": "./dist/lib/adapters/sqlserver/index.js"
    },
    "./mysql": {
      "development": "./src/lib/adapters/mysql/index.ts",
      "types": "./dist/lib/adapters/mysql/index.d.ts",
      "import": "./dist/lib/adapters/mysql/index.js",
      "default": "./dist/lib/adapters/mysql/index.js"
    },
    "./synthesis": {
      "development": "./src/lib/synthesis/index.ts",
      "types": "./dist/lib/synthesis/index.d.ts",
      "import": "./dist/lib/synthesis/index.js",
      "default": "./dist/lib/synthesis/index.js"
    }
  },
  "files": [
    "dist",
    "!**/*.tsbuildinfo",
    "!**/*.test.*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@ai-sdk/groq": "2.0.26",
    "@deepagents/agent": "0.6.0",
    "@deepagents/toolbox": "0.6.0",
    "ai": "^5.0.102",
    "autoevals": "^0.0.131",
    "dedent": "1.7.0",
    "evalite": "^0.19.0",
    "openai": "5.12.2",
    "p-limit": "^6.2.0",
    "pluralize": "^8.0.0",
    "uuid": "^13.0.0",
    "zod": "^3.25.76 || ^4.0.0",
    "chalk": "^5.6.0",
    "@ai-sdk/openai": "2.0.76",
    "nano-spawn": "^1.0.3",
    "p-retry": "^7.1.1"
  }
}
