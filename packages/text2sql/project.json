{
  "name": "@deepagents/text2sql",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/text2sql/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/esbuild:esbuild",
      "outputs": ["{options.outputPath}"],
      "options": {
        "additionalEntryPoints": [
          "packages/text2sql/src/lib/instructions.ts",
          "packages/text2sql/src/lib/adapters/groundings/index.ts",
          "packages/text2sql/src/lib/adapters/sqlite/index.ts",
          "packages/text2sql/src/lib/adapters/postgres/index.ts",
          "packages/text2sql/src/lib/adapters/sqlserver/index.ts",
          "packages/text2sql/src/lib/synthesis/index.ts"
        ]
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node --test",
        "cwd": "{projectRoot}"
      }
    },
    "eval": {
      "executor": "nx:run-commands",
      "dependsOn": ["build"],
      "options": {
        "command": "evalite",
        "cwd": "{projectRoot}",
        "envFile": ".env"
      }
    },
    "eval-serve": {
      "dependsOn": ["build"],
      "executor": "nx:run-commands",
      "options": {
        "command": "evalite serve",
        "cwd": "{projectRoot}",
        "envFile": ".env"
      }
    },
    "eval-debug": {
      "dependsOn": ["build"],
      "executor": "nx:run-commands",
      "options": {
        "command": "node --no-warnings src/evals/debug-eval.ts",
        "cwd": "{projectRoot}",
        "envFile": ".env"
      }
    }
  }
}
